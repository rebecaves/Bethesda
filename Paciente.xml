        <dataField fieldName="IDMunicipio" aliasFieldName="TheidMunicipiodescripcion" />
        <dataField fieldName="IDTurno" aliasFieldName="IDTurdescripcion" />
        <dataField fieldName="IDAcceso" aliasFieldName="TheidAccesodescripcion" />
